{
    "language": "Spanish",
    "rules": {
        "language-links-es": {
            "expression": "\\[\\[(?!vdc:|:?file:|w(ikia|ikipedia)?:|#|:?image:|media:)([^/]+?)(\\/(?!es).+]]|]]|#)",
            "flags": "gi",
            "reason": "Link to wrong language (missing /es)"
        },
        "wikipedia-links-es": {
            "expression": "\\[\\[w(ikipedia)?:(?!es:)((.*)|(.*))\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong Wikipedia (missing es: after w:)"
        }
    }
}