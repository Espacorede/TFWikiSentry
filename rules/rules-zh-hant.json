{
    "language": "Traditional Chinese",
    "rules": {
        "language-links-hant": {
            "expression": "\\[\\[(?!vdc:|w(?:ikia|ikipedia)?:|file|image|media:)([^\\]|\/]+?(?!\/zh-hant))(#.*)?(\\|.*?)?\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong language (missing /zh-hant)"
        },
        "wikipedia-links-hant": {
            "expression": "\\[\\[w(ikipedia)?:(?!zh:)((.*)|(.*))\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong Wikipedia (missing zh: after w:)"
        }
    }
}