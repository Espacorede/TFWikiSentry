{
    "language": "Korean",
    "rules": {
        "language-links-ko": {
            "expression": "\\[\\[(?!vdc:|:?file:|w(ikia|ikipedia)?:|#|:?image:|media:)([^/]+?)(\\/(?!ko).+]]|]]|#)",
            "flags": "gi",
            "reason": "Link to wrong language (missing /ko)"
        },
        "wikipedia-links-ko": {
            "expression": "\\[\\[w(ikipedia)?:(?!ko:)((.*?)|(.*))\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong Wikipedia (missing ko: after w:)"
        }
    }
}