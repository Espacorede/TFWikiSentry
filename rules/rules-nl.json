{
    "language": "Dutch",
    "rules": {
        "language-links-nl": {
            "expression": "\\[\\[(?!vdc:|w(?:ikia|ikipedia)?:|file:)([^\\]|\/]+?(?!\/nl))(#.*)?(\\|.*?)?\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong language (missing /nl)"
        },
        "wikipedia-links-nl": {
            "expression": "\\[\\[w(ikipedia)?:(?!nl:)((.*)|(.*))\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong Wikipedia (missing nl: after w:)"
        }
    }
}