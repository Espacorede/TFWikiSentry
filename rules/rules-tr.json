{
    "language": "Turkish",
    "rules": {
        "language-links-tr": {
            "expression": "\\[\\[(?!vdc:|w(?:ikia|ikipedia)?:|file|image|media:)([^\\]|\/]+?(?!\/tr))(#.*)?(\\|.*?)?\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong language (missing /tr)"
        },
        "wikipedia-links-tr": {
            "expression": "\\[\\[w(ikipedia)?:(?!tr:)((.*)|(.*))\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong Wikipedia (missing tr: after w:)"
        }
    }
}