{
    "language": "Hungarian",
    "rules": {
        "language-links-hu": {
            "expression": "\\[\\[(?!vdc:|w(?:ikia|ikipedia)?:|file|image|media:)([^\\]|\/]+?(?!\/hu))(#.*)?(\\|.*?)?\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong language (missing /hu)"
        },
        "wikipedia-links-hu": {
            "expression": "\\[\\[w(ikipedia)?:(?!hu:)((.*)|(.*))\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong Wikipedia (missing hu: after w:)"
        }
    }
}