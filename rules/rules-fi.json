{
    "language": "Finnish",
    "rules": {
        "language-links-fi": {
            "expression": "\\[\\[(?!vdc:|:?file:|w(ikia|ikipedia)?:|#|:?image:|media:)([^/]+?)(\\/(?!fi).+]]|]]|#)",
            "flags": "gi",
            "reason": "Link to wrong language (missing /fi)"
        },
        "wikipedia-links-fi": {
            "expression": "\\[\\[w(ikipedia)?:(?!fi:)((.*?)|(.*))\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong Wikipedia (missing fi: after w:)"
        }
    }
}