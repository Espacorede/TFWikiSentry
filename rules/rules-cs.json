{
    "language": "Czech",
    "rules": {
        "language-links-cs": {
            "expression": "\\[\\[(?!vdc:|w(?:ikia|ikipedia)?:|file|image|media:)([^\\]|\/]+?(?!\/cs))(#.*)?(\\|.*?)?\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong language (missing /cs)"
        },
        "wikipedia-links-cs": {
            "expression": "\\[\\[w(ikipedia)?:(?!cs:)((.*)|(.*))\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong Wikipedia (missing cs: after w:)"
        }
    }
}