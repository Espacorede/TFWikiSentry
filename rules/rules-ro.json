{
    "language": "Romanian",
    "rules": {
        "language-links-ro": {
            "expression": "\\[\\[(?!vdc:|w(?:ikia|ikipedia)?:|file|image|media:)([^\\]|\/]+?(?!\/ro))(#.*)?(\\|.*?)?\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong language (missing /ro)"
        },
        "wikipedia-links-ro": {
            "expression": "\\[\\[w(ikipedia)?:(?!ro:)((.*)|(.*))\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong Wikipedia (missing ro: after w:)"
        }
    }
}