{
    "language": "French",
    "rules": {
        "language-links-fr": {
            "expression": "\\[\\[(?!vdc:|:?file:|w(ikia|ikipedia)?:|#|:?image:|media:)([^/]+?)(\\/(?!fr).+]]|]]|#)",
            "flags": "gi",
            "reason": "Link to wrong language (missing /fr)"
        },
        "wikipedia-links-fr": {
            "expression": "\\[\\[w(ikipedia)?:(?!fr:)((.*)|(.*))\\]\\]",
            "flags": "gi",
            "reason": "Link to wrong Wikipedia (missing fr: after w:)"
        }
    }
}